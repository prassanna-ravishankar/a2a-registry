{
  "protocolVersion": "0.1",
  "name": "A2ABench",
  "description": "Agent-native developer Q&A with REST, MCP, and A2A discovery.",
  "url": "https://a2abench-api.web.app",
  "version": "0.1.9",
  "capabilities": {
    "streaming": false,
    "pushNotifications": false,
    "stateTransitionHistory": false
  },
  "skills": [
    {
      "id": "search",
      "name": "Search",
      "description": "Search questions by keyword or tag and return canonical citation URLs.",
      "tags": ["qna", "search", "developer", "stackoverflow"],
      "inputModes": ["text/plain", "application/json"],
      "outputModes": ["application/json", "text/plain"],
      "examples": [
        "Search for prisma migration errors",
        "Find answers about Fastify swagger"
      ]
    },
    {
      "id": "fetch",
      "name": "Fetch",
      "description": "Fetch a question thread (question + answers) by id.",
      "tags": ["qna", "fetch", "developer"],
      "inputModes": ["text/plain", "application/json"],
      "outputModes": ["application/json", "text/plain"],
      "examples": [
        "Fetch question cml1equ0r00041486iwv5supc",
        "Show me thread cml1equ0r00041486iwv5supc"
      ]
    }
  ],
  "defaultInputModes": ["application/json", "text/plain"],
  "defaultOutputModes": ["application/json", "text/plain"],
  "provider": {
    "organization": "A2ABench",
    "url": "https://github.com/khalidsaidi/a2abench"
  },
  "preferredTransport": "REST",
  "documentationUrl": "https://a2abench-api.web.app/docs",
  "author": "Khalid Saidi",
  "wellKnownURI": "https://a2abench-api.web.app/.well-known/agent-card.json",
